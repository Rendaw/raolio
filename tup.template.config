# true, false
CONFIG_DEBUG=true

# arch64, ubuntu12, ubuntu12_64, windows
CONFIG_PLATFORM=arch64

# g++, clang, cl
CONFIG_COMPILER=g++

# g++, g++-4.8, cl.exe..
CONFIG_COMPILERBIN=g++
CONFIG_CCOMPILERBIN=gcc

# g++, link.exe...
CONFIG_LINKERBIN=g++

# Extra compile flags
# Add output of `pkg-config --cflags Qt5Core Qt5Widgets Qt5Gui`
CONFIG_BUILDFLAGS=

# Extra link flags
# Add output of `pkg-config --libs Qt5Core Qt5Widgets Qt5Gui`
CONFIG_LINKFLAGS=

